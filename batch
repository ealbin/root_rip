for f in `more todo` 
do
    rm -f ${f%.*}.batchlog 2>&1 /dev/null
    cp rip/$f rip_snapshot/$f
    echo "begin quality control"
    nice -10 python qualitycontrol.py rip_snapshot/$f |& tee -a ${f%.*}.batchlog
    echo "begin probe"
    nice -10 python probe.py rip_snapshot/$f |& tee -a ${f%.*}.batchlog
    echo "begin morestats"
    nice -10 python morestats.py rip_snapshot/$f |& tee -a ${f%.*}.batchlog
done
